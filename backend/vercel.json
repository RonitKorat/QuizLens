{
    "version":2,
    "builds":[{"src":  "./index.js","use":"@vercel/node"}],
    "routes":[{"src":"/(.*)","dest":"./index.js"}],
    "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; style-src 'self' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com;"
        }
      ]
    }
  ]
}